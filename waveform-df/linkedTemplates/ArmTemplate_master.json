{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "waveform-df"
		},
		"data_blob_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://waveformstorage1234.blob.core.windows.net/"
		},
		"service_waveform_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "waveform-sql-server.database.windows.net"
		},
		"service_waveform_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "waveform_db"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "waveform-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"data_blob_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('data_blob_properties_typeProperties_serviceEndpoint')]"
					},
					"service_waveform_db_properties_typeProperties_server": {
						"value": "[parameters('service_waveform_db_properties_typeProperties_server')]"
					},
					"service_waveform_db_properties_typeProperties_database": {
						"value": "[parameters('service_waveform_db_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}